{
  "name": "wp-conf-01",
  "version": "1.0.0",
  "description": "Webpack config for typescript project with: babel, eslint, prettier, jest, puppeteer.",
  "private": true,
  "scripts": {
    "build:dev": "webpack-dev-server --config webpack.dev.config.js --env dev --colors",
    "build:prod": "webpack --config webpack.prod.config.js --env prod --colors",
    "test": "jest --watchAll --colors --config=jest.config.js",
    "build:dev:test": "jest --colors && webpack-dev-server --config webpack.dev.config.js --env dev --colors",
    "e2e": "jest e2e/app.test.ts --config=puppeteer.config.js"
  },
  "keywords": [
    "webpack",
    "typescript",
    "babel",
    "eslint",
    "prettier",
    "jest",
    "puppeteer"
  ],
  "author": "Kacper Kopa≈Ñski",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "puppeteer": "^2.1.1",
    "sass": "^1.26.2",
    "sass-loader": "^8.0.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "core-js": "^3.6.4"
  }
}
